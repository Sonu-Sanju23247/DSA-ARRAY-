//Write a programm to calculate the fibbonacci series:

public class  Main{
    static int fibonnaci_calculation(int n){
        if(n==0 || n==1){
            return n;
        }
        int prevfirst=fibonnaci_calculation(n-1);
        int previousSecond=fibonnaci_calculation(n-2);
        return prevfirst+previousSecond;
    }
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        int x=sc.nextInt();
        int fibonacciseries=fibonnaci_calculation(x);
        System.out.println(fibonacciseries);

    }
}

------------------and to print to nth term-----------
psvm{
for(int i=0;i<=10;i++){
System.oout.println(fibonnaci_calculation[i]);
}
}
