// 12. Given an array "a" consisting of integer. return the first value that is repeating in this array.
        if no value is being repeated return -1.

public class Main{
    static int firstrepeat(int arr[]) {
        for (int i = 0; i < arr.length; i++) {
            for (int j = 1; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    return arr[i];
                }
            }

        }
        return -1;
    }
    public static  void main (String[]args){
        int arr[]={1,2,3,4,2,3,5};
        System.out.println(firstrepeat(arr));

    }
        }

